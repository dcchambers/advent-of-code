package main

import (
    "bufio"
    "fmt"
    "os"
)

func getArrayLength(f *os.File) int {
    length := 0
    scanner := bufio.NewScanner(f)
    for scanner.Scan() {
        length++
    }
    return length
}

func main() {

    data,err := os.Open("data.txt")
    
    if err != nil {
        panic(err)
    }

    defer data.Close()

    fmt.Println(getArrayLengt(data))
}
